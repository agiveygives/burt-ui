{
  "name": "burt-ui",
  "version": "0.1.0",
  "description": "React component library",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "ts-declarations": "tsc --emitDeclarationOnly",
    "prebuild": "rm -rf dist && rm -rf types && npm run ts-declarations",
    "rollup-watch": "rollup --bundleConfigAsCjs -c -w",
    "build": "npm run prebuild && rollup -c --bundleConfigAsCjs",
    "prepublish": "npm run build",
    "test": "jest --watchAll",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "author": "Andrew Givens",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
