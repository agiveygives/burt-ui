    "@storybook/react-webpack5": "^7.0.0-beta.44",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.0.27",
    "babel-loader": "^8.3.0",
    "classnames": "^2.3.2",
    "css-modules-require-hook": "^4.2.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-transform-css": "^6.0.1",
    "polished": "^4.2.2",
    "react": "^18.2.0",
